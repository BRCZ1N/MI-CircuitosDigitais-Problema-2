module modulo_contador_sync_6_bits(clr, button_clk, q);

	input button_clk,clr;
	output [5:0] q;
	wire ff1_and_ff2,ff1_ff2_ff3,ff1_ff2_ff3_ff4,ff1_ff2_ff3_ff4_ff5;
	
	modulo_ff_t ff_1(.t(1'b1),.clk(button_clk),.clr(clr),.q(q[0]),);
	modulo_ff_t ff_2(.t(q[0]),.clk(button_clk),.clr(clr),.q(q[1]),);
	
	and(ff1_and_ff2, q[1],q[0]);
	
	modulo_ff_t ff_3(.t(ff1_and_ff2),.clk(clk),.clr(clr),.q(q[2]),);
	
	and(ff1_ff2_ff3,ff1_and_ff2, f3);
	
	modulo_ff_t ff_2(.t(q[0]),.clk(button_clk),.clr(clr),.q(q[1]),);
	
	and(ff1_ff2_ff3_ff4,ff1_ff2_ff3, f4);
	
	modulo_ff_t ff_2(.t(q[0]),.clk(button_clk),.clr(clr),.q(q[1]),);
	
	and(ff1_ff2_ff3_ff4_ff5,ff1_ff2_ff3_ff4, f5);
	
	modulo_ff_t ff_2(.t(q[0]),.clk(button_clk),.clr(clr),.q(q[1]),);
	
	

	
endmodule 
